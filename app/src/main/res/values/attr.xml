<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomSwitch">
        <attr name="custom_switch_title" format="string" />
        <attr name="custom_switch_subtitle" format="string" />
        <attr name="custom_switch_enabled" format="boolean" />
        <attr name="custom_switch_checked" format="boolean" />
        <attr name="custom_switch_state" format="boolean" />
        <attr name="android:enabled"/>
    </declare-styleable>

    <declare-styleable name="CustomEditText">
        <attr name="custom_edit_text_max_line" format="integer"/>
        <attr name="custom_edit_text_title" format="string"/>
        <attr name="custom_edit_text_placeholder" format="string"/>
        <attr name="custom_edit_icon_left" format="integer"/>
        <attr name="custom_edit_action_custom" format="boolean"/>
    </declare-styleable>
</resources>